# Rename this file to .env and fill in values
# Set HEADLESS=true to run without opening the browser window
HEADLESS=false
# High-level concurrency
BROWSERS=1
TABS_PER_BROWSER=1

# Supabase project
SUPABASE_URL=https://YOUR_PROJECT_ID.supabase.co
# Prefer service role key for server-side inserts; never expose publicly
SUPABASE_SERVICE_ROLE=YOUR_SERVICE_ROLE_KEY
# or supply a generic key (falls back to anon if given)
# SUPABASE_KEY=
# SUPABASE_ANON_KEY=

# Target table name
SUPABASE_TABLE=emails

# Optional NopeCHA CAPTCHA solver (official)
# IMPORTANT: Use the API key from the NopeCHA dashboard (NOT a Stripe subscription id like sub_...)
# Primary (official) env var name:
# NOPECHA_KEY=YOUR_NOPECHA_API_KEY
# Legacy/compat:
# NOPECHA_API_KEY=YOUR_NOPECHA_API_KEY
# If you want to load the NopeCHA browser extension (Chromium only, requires headless=false):
# NOPECHA_EXTENSION_PATH=C:\\path\\to\\nopecha-extension
